{
  "name": "maatwebsite/excel21",
  "description": "Supercharged Excel21 exports in Laravel",
  "license": "MIT",
  "keywords": [
    "laravel",
    "phpexcel",
    "excel",
    "csv",
    "export",
    "import",
    "batch"
  ],
  "authors": [
    {
      "name": "Maatwebsite.nl",
      "email": "patrick@maatwebsite.nl"
    }
  ],
  "require": {
    "php": "^8.3",
    "phpoffice/phpexcel": "^1.8.1",
    "illuminate/cache": "^11.0",
    "illuminate/config": "^11.0",
    "illuminate/filesystem": "^11.0",
    "illuminate/support": "^11.0",
    "jeremeamia/superclosure": "^2.4",
    "nesbot/carbon": "^2",
    "tijsverkoyen/css-to-inline-styles": "~2.0"
  },
  "autoload": {
    "classmap": [
      "src/Maatwebsite/Excel21"
    ],
    "psr-0": {
      "Maatwebsite\\Excel21\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "extra": {
        "laravel": {
            "providers": [
                "Maatwebsite\\Excel21\\ExcelServiceProvider"
            ],
            "aliases": {
                "Excel21": "Maatwebsite\\Excel21\\Facades\\Excel21"
            }
        }
    },
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": true
    }
  }
}
